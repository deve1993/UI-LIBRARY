{
  "name": "@deve1993/ui-library",
  "version": "1.0.0",
  "description": "Component library con UI components e sezioni riutilizzabili creata con sistema Library Agent",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs",
      "require": "./dist/components.js"
    },
    "./styles": "./styles/globals.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "styles",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deve1993/UI-LIBRARY.git"
  },
  "bugs": {
    "url": "https://github.com/deve1993/UI-LIBRARY/issues"
  },
  "homepage": "https://github.com/deve1993/UI-LIBRARY#readme",
  "author": "deve1993",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "react",
    "vue",
    "svelte",
    "components",
    "ui-library",
    "design-system",
    "tailwind",
    "typescript",
    "component-library",
    "ui-components",
    "web-components"
  ],
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:stories": "test-storybook",
    "build": "tsup",
    "prepublishOnly": "npm run build && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate-components": "tsx scripts/validate-components.ts",
    "generate-component": "tsx scripts/generate-component.ts",
    "generate:story": "tsx scripts/generate-story.ts",
    "check-dependencies": "tsx scripts/check-dependencies.ts",
    "check-dependencies:fix": "tsx scripts/check-dependencies.ts --fix",
    "update-registry": "tsx scripts/update-registry.ts",
    "precommit": "npm run validate-components && npm run check-dependencies && npm run type-check",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.0",
    "@storybook/addon-essentials": "^7.6.0",
    "@storybook/addon-interactions": "^7.6.0",
    "@storybook/addon-links": "^7.6.0",
    "@storybook/react": "^7.6.0",
    "@storybook/react-vite": "^7.6.0",
    "@storybook/test-runner": "^0.16.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.19.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.6.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.4.5",
    "husky": "^8.0.0",
    "jsdom": "^23.2.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "storybook": "^7.6.0",
    "tailwindcss": "^3.4.0",
    "ts-morph": "^21.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "tailwind-merge": "^3.3.1"
  }
}
