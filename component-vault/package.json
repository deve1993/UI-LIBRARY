{
  "name": "@deve1993/ui-library",
  "version": "1.0.0",
  "description": "Component library con UI components e sezioni riutilizzabili creata con sistema Library Agent",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs",
      "require": "./dist/components.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.js"
    },
    "./sections": {
      "types": "./dist/sections.d.ts",
      "import": "./dist/sections.mjs",
      "require": "./dist/sections.js"
    },
    "./sections/hero": {
      "types": "./dist/sections-hero.d.ts",
      "import": "./dist/sections-hero.mjs",
      "require": "./dist/sections-hero.js"
    },
    "./sections/features": {
      "types": "./dist/sections-features.d.ts",
      "import": "./dist/sections-features.mjs",
      "require": "./dist/sections-features.js"
    },
    "./sections/footer": {
      "types": "./dist/sections-footer.d.ts",
      "import": "./dist/sections-footer.mjs",
      "require": "./dist/sections-footer.js"
    },
    "./sections/cta": {
      "types": "./dist/sections-cta.d.ts",
      "import": "./dist/sections-cta.mjs",
      "require": "./dist/sections-cta.js"
    },
    "./sections/navigation": {
      "types": "./dist/sections-navigation.d.ts",
      "import": "./dist/sections-navigation.mjs",
      "require": "./dist/sections-navigation.js"
    },
    "./sections/social-proof": {
      "types": "./dist/sections-social-proof.d.ts",
      "import": "./dist/sections-social-proof.mjs",
      "require": "./dist/sections-social-proof.js"
    },
    "./sections/pricing": {
      "types": "./dist/sections-pricing.d.ts",
      "import": "./dist/sections-pricing.mjs",
      "require": "./dist/sections-pricing.js"
    },
    "./sections/contact": {
      "types": "./dist/sections-contact.d.ts",
      "import": "./dist/sections-contact.mjs",
      "require": "./dist/sections-contact.js"
    },
    "./sections/content": {
      "types": "./dist/sections-content.d.ts",
      "import": "./dist/sections-content.mjs",
      "require": "./dist/sections-content.js"
    },
    "./effects": {
      "types": "./dist/effects.d.ts",
      "import": "./dist/effects.mjs",
      "require": "./dist/effects.js"
    },
    "./styles": "./styles/globals.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "styles",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/deve1993/UI-LIBRARY.git"
  },
  "bugs": {
    "url": "https://github.com/deve1993/UI-LIBRARY/issues"
  },
  "homepage": "https://github.com/deve1993/UI-LIBRARY#readme",
  "author": "deve1993",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "react",
    "vue",
    "svelte",
    "components",
    "ui-library",
    "design-system",
    "tailwind",
    "typescript",
    "component-library",
    "ui-components",
    "web-components"
  ],
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:stories": "test-storybook",
    "build": "tsup",
    "prepublishOnly": "npm run build && npm run test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "validate-components": "tsx scripts/validate-components.ts",
    "generate-component": "tsx scripts/generate-component.ts",
    "generate:story": "tsx scripts/generate-story.ts",
    "generate:test": "tsx scripts/generate-test.ts",
    "generate:test:all": "tsx scripts/generate-test.ts generate --all",
    "generate:test:priority": "tsx scripts/generate-test.ts generate --priority high",
    "check-dependencies": "tsx scripts/check-dependencies.ts",
    "check-dependencies:fix": "tsx scripts/check-dependencies.ts --fix",
    "update-registry": "tsx scripts/update-registry.ts",
    "migrate:audit": "tsx scripts/migrate-dependencies.ts audit",
    "migrate:plan": "tsx scripts/migrate-dependencies.ts plan",
    "migrate:safe": "tsx scripts/migrate-dependencies.ts migrate --safe",
    "migrate:all": "tsx scripts/migrate-dependencies.ts migrate --all",
    "performance:audit": "tsx scripts/analyze-performance.ts",
    "performance:bundle": "tsx scripts/analyze-performance.ts bundle",
    "performance:renders": "tsx scripts/analyze-performance.ts renders",
    "a11y:audit": "tsx scripts/audit-accessibility.ts",
    "a11y:fix": "tsx scripts/audit-accessibility.ts fix",
    "a11y:test": "tsx scripts/audit-accessibility.ts test",
    "release:prepare": "tsx scripts/release.ts prepare",
    "release:preview": "tsx scripts/release.ts preview",
    "release:publish": "tsx scripts/release.ts publish",
    "quality:analyze": "tsx scripts/code-quality.ts analyze",
    "quality:fix-eslint": "tsx scripts/code-quality.ts fix-eslint",
    "quality:cleanup": "tsx scripts/code-quality.ts cleanup",
    "quality:debt": "tsx scripts/code-quality.ts debt",
    "visual:baseline": "tsx scripts/visual-regression.ts baseline",
    "visual:test": "tsx scripts/visual-regression.ts test",
    "visual:ci": "tsx scripts/visual-regression.ts ci",
    "visual:status": "tsx scripts/visual-regression.ts status",
    "visual:list": "tsx scripts/visual-regression.ts list",
    "security:audit": "tsx scripts/security-scanner.ts audit",
    "security:vulns": "tsx scripts/security-scanner.ts vulns",
    "security:licenses": "tsx scripts/security-scanner.ts licenses",
    "security:secrets": "tsx scripts/security-scanner.ts secrets",
    "security:fix": "tsx scripts/security-scanner.ts fix",
    "security:report": "tsx scripts/security-scanner.ts report",
    "cicd:init": "tsx scripts/cicd-pipeline.ts init",
    "cicd:validate": "tsx scripts/cicd-pipeline.ts validate",
    "cicd:test": "tsx scripts/cicd-pipeline.ts test",
    "cicd:deploy-storybook": "tsx scripts/cicd-pipeline.ts deploy-storybook",
    "cicd:status": "tsx scripts/cicd-pipeline.ts status",
    "precommit": "npm run validate-components && npm run check-dependencies && npm run type-check",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.2",
    "@playwright/test": "^1.56.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.19.23",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.4",
    "@vitest/ui": "^4.0.4",
    "autoprefixer": "^10.4.0",
    "chromatic": "^13.3.3",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.4.5",
    "husky": "^8.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^23.2.0",
    "playwright": "^1.40.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.0",
    "ts-morph": "^21.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0",
    "vite": "^7.1.12",
    "vitest": "^4.0.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.546.0",
    "tailwind-merge": "^3.3.1"
  }
}
